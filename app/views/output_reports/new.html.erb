<h1>アウトプットレポート</h1>

<%= form_with model: @report, html: {class: "form-container"} do |form| %> 
<div class="form-input">
  <%= form.label :"キーワード" %> <%= form.text_field :title %>
</div>

<div class="form-input">
  <%= form.label :"内容" %> <%= form.text_area :content %>
</div>

<div class="button-container">
  <%= form.submit "投稿", class: "link-button" %>
</div>
<% end %>